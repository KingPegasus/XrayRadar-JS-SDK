{
  "name": "@xrayradar/node",
  "version": "0.2.0",
  "description": "XrayRadar SDK for Node.js",
  "keywords": [
    "xrayradar",
    "error-tracking",
    "observability",
    "monitoring",
    "sdk",
    "node",
    "nodejs",
    "javascript",
    "typescript"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "test": "vitest run --passWithNoTests",
    "test:coverage": "vitest run --coverage --passWithNoTests",
    "lint": "eslint src",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@xrayradar/core": "*"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.3",
    "vitest": "^4.0.0",
    "@types/node": "^20.10.0"
  },
  "engines": {
    "node": ">=20"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KingPegasus/XrayRadar-JS-SDK.git"
  }
}
